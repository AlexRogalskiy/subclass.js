{
  "name": "subclass",
  "description": "Inherit, extend, decorate, and override classes and instances.",
  "version": "1.0.3",
  "author": "Jamie Mason (https://github.com/JamieMason)",
  "bugs": "https://github.com/JamieMason/subclass.js/issues",
  "devDependencies": {
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "tslint": "5.17.0",
    "typescript": "3.5.2"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/JamieMason/subclass.js",
  "keywords": [
    "decorate",
    "decorator",
    "extend",
    "extendable",
    "inherit",
    "inheritance",
    "mixin",
    "prototypal",
    "prototype",
    "statics",
    "subclass"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "JamieMason/subclass.js",
  "scripts": {
    "build": "tsc --project .",
    "format": "prettier --write 'README.md' './src/**/*.{ts}'",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prebuild": "rimraf dist",
    "prelint": "npm run format",
    "prepack": "npm run build"
  }
}
